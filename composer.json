{
    "name": "funnydevjsc/minio-laravel-integrate",
    "description": "Laravel package for MinIO",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "FunnyDev\\MinIO\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Funny dev., Jsc",
            "email": "contact@funnydev.vn",
            "homepage": "https://github.com/funnydevjsc"
        }
    ],
    "homepage": "https://github.com/funnydevjsc/minio-laravel-integrate",
    "keywords": ["Laravel", "package", "minio"],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "orchestra/testbench": "^v8.27.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FunnyDev\\MinIO\\MinIOServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
